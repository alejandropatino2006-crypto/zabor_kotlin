  "scripts": {
    "ng": "ng",
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "teststart": "ng serve -c testing",
    "build": "ng build --prod",
    "dockerbuild": "docker run --rm -it -w /app -v /mnt/e/_WORKS/zabor/zabor-app/site:/app node:14.21.3-alpine sh -c \"cd /app && npm run build\"",
    "gitbuild": "ng build --prod --base-href /start-angular/SB-Admin-BS4-Angular-7/master/dist/",
    "stagingbuild": "ng build -c staging",
    "dockerstagingbuild": "docker run --rm -it -w /app -v /mnt/e/_WORKS/zabor/zabor-app/site:/app node:14.21.3-alpine sh -c \"cd /app && npm run stagingbuild\"",
    "test": "ng test",
    "test-ci": "TEST_CI=true ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "staging0": "ng serve -c staging --host 0.0.0.0 --port 8105 --disableHostCheck",
    "staging": "ng serve -c staging --host 0.0.0.0 --port 4202 --disableHostCheck",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "devUmar": "SET NODE_OPTIONS=--openssl-legacy-provider && ng serve --ssl true --ssl-key ../certs/localhost.test-key.pem --ssl-cert ../certs/localhost.test.pem -c umar --host localhost.test --port 4201 --disableHostCheck",
    "e2etestsNotWorking": "cross-env TS_NODE_PROJECT='./tsconfig.e2e.json' npx codeceptjs run --verbose -c ./e2e/codecept.conf.ts",
    "pree2etests": "shx mv ./tsconfig.json ./tsconfig.json.txt",
    "e2etests": "npx codeceptjs run --verbose -c ./e2e/codecept.conf.ts",
    "poste2etests": "shx mv ./tsconfig.json.txt ./tsconfig.json"
  },
